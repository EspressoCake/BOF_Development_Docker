{
	"name": "svrt_musl_image",
	"build": {
		"dockerfile": "Dockerfile",
		"context": ".."
	},
	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash",
		"C_Cpp.default.compilerPath": "/usr/bin/x86_64-w64-mingw32-gcc"
	},

	"customizations": {
		"vscode": {
			"extensions": [
				"ms-vscode.cpptools",
				"ms-vscode.cpptools-extension-pack"
			]
		}
	}
}
